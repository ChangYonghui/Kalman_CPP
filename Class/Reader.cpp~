#include "../Headers/Reader.h"
#include <iostream>
#include <string>

using namespace std;

Reader::Reader(std::string n){      
        this->data = new vector <int>(6);
        this->topic_name=n;
}

//Affichage des données dans le buffer
void GPS::Display(){

        cout << "Nom du topic à lire : " << this->topic_name << endl;
        for(int i=0; i<6; i++){
                cout << this->data->at(i) << endl;
        }

}
