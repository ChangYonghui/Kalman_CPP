Programme : main.o DataReader.o Global.o GPS.o IMU.o QR.o
        g++ main.o DataReader.o Global.o GPS.o IMU.o QR.o -o Programme
 
main.o : main.cpp DataReader.cpp Global.cpp GPS.cpp IMU.cpp QR.cpp
        g++ -cpp main.cpp -o main.o
 
DataReader.o : =DataReader.cpp
        g++ -cpp DataReader.cpp -o DataReader.o
        
Global.o : =Global.cpp
        g++ -cpp Global.cpp -o Global.o
      
GPS.o : =GPS.cpp
        g++ -cpp GPS.cpp -o GPS.o
        
IMU.o : =IMU.cpp
        g++ -cpp IMU.cpp -o IMU.o
        
QR.o : =QR.cpp
        g++ -cpp QR.cpp -o QR.o
